<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171">
   <children>
      <VBox alignment="CENTER" prefHeight="441.0" prefWidth="521.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Flight ID of Flight to Book">
               <font>
                  <Font size="18.0" />
               </font>
            </Text>
            <TextField fx:id="BFID" alignment="CENTER" prefHeight="61.0" prefWidth="521.0" promptText="Flight ID">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <Button fx:id="MainMenu" mnemonicParsing="false" prefHeight="98.0" prefWidth="165.0" text="Book">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
